{
  "config": {
    "name": "basic_config"
  },
  "basic": {
    "experiment_name": "baseline",
    "model": "auto_encoder_flat",
    "result_directory": "",
    "mode": "train"
  },
  "preprocessing": {
    "dataset": {
      "path": "1K/trainB",
      "training_size": 7500,
      "validation_size": 1800
    },
    "dataloader": {
      "batch_size": 32,
      "shuffle": "True",
      "num_workers": 6
    }
  },
  "training": {
    "epochs": 1500,
    "save_frequency": 50,
    "optimizer": {
      "type": "Adam",
      "learning_rate": 0.01,
      "momentum": 0.9
    },
    "criterion": {
      "type": "BCELoss"
    },
    "lr_scheduler": {
      "type": "StepLR",
      "step_size": 20,
      "gamma": 0.75
    }
  },
  "evaluation": {
  }
}

