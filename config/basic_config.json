{
  "config": {
    "name": "basic_config"
  },
  "basic": {
    "experiment_name": "baseline",
    "experiment_description": "sample description",
    "model": "resnet_51",
    "pretrained": 1,
    "result_directory": "/home/noah/Desktop/affact/PyAffact/PyAffact/experiments/results/2020-11-19-16-04-46-baseline",
    "result_directory_name": "2020-11-19-16-04-46-baseline",
    "mode": "train",
    "enable_wand_reporting": 1
  },
  "training": {
    "epochs": 3,
    "save_frequency": 2,
    "optimizer": {
      "type": "SGD",
      "learning_rate": 0.1,
      "momentum": 0.9
    },
    "criterion": {
      "type": "BCEWithLogitsLoss"
    },
    "lr_scheduler": {
      "type": "StepLR",
      "step_size": 7,
      "gamma": 0.1
    }
  },
  "preprocessing": {
    "dataset": {
      "train_fraction": 0.5,
      "val_fraction": 0.2,
      "test_fraction": 0.3,
      "number_of_samples": 100,
      "dataset_labels_filename": "CelebA/list_attr_celeba.txt",
      "dataset_image_folder": "CelebA/Img100",
      "uses_landmarks": 1,
      "landmarks_filename": "CelebA/list_landmarks_celeba.txt"
    },
    "dataloader": {
      "batch_size": 16,
      "shuffle": "True",
      "num_workers": 6
    },
    "transformation": {
      "use_affact_transformator": 1,
      "save_transformation_image": {
        "enabled": 1,
        "frequency": 10
      },
      "crop_size": {
        "x": 224,
        "y": 224
      },
      "random_bounding_box": {
        "enabled": 0
      },
      "scale_jitter": {
        "enabled": 1,
        "normal_distribution": {
          "mean": 1,
          "std": 0.1
        }
      },
      "angle_jitter": {
        "enabled": 1,
        "normal_distribution": {
          "mean": 0,
          "std": 20
        }
      },
      "shift_jitter": {
        "enabled": 1,
        "normal_distribution": {
          "mean": 0,
          "std": 0.05
        }
      },
      "mirror": {
        "enabled": 1,
        "probability": 0.5
      },
      "gaussian_blur": {
        "enabled": 1,
        "normal_distribution": {
          "mean": 0,
          "std": 3
        }
      },
      "gamma": {
        "enabled": 1,
        "normal_distribution": {
          "mean": 0,
          "std": 1
        }
      }
    }
  },
  "evaluation": {
    "test_labels_pickle_filename": "test_labels.pkl",
    "test_landmarks_pickle_filename": "test_landmarks.pkl",
    "train_majority_pickle_filename": "train_majority.pkl",
    "model_weights_filename": "best-3.pt",
    "qualitative": {
      "number_of_samples": 5
    },
    "quantitative": {
      "overall_accuracy": 1,
      "per_attribute_accuracy": 1
    }
  }
}