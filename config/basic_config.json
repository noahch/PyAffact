{
  "config": {
    "name": "basic_config"
  },
  "basic": {
    "cuda_device_name": "cuda:0",
    "experiment_name": "baseline on own network",
    "experiment_description": "sample description",
    "model": "resnet_noahAndYves",
    "pretrained": 1,
    "result_directory": "/home/noah/Desktop/affact/PyAffact/PyAffact/experiments/results",
    "result_directory_name": "2020-12-01-13-17-07-baseline",
    "mode": "train",
    "enable_wand_reporting": 0
  },
  "training": {
    "epochs": 2,
    "save_frequency": 10,
    "optimizer": {
      "type": "SGD",
      "learning_rate": 0.1,
      "momentum": 0
    },
    "criterion": {
      "type": "BCEWithLogitsLoss"
    },
    "lr_scheduler": {
      "type": "ReduceLROnPlateau",
      "step_size": 7,
      "gamma": 0.1,
      "patience": 4
    },
    "dropout": 0.2
  },
  "preprocessing": {
    "dataset": {
      "use_partition_file": 1,
      "partition_filename": "dataset/CelebA/list_eval_partition.txt",
      "train_fraction": 0.5,
      "val_fraction": 0.2,
      "test_fraction": 0.3,
      "number_of_samples": -1,
      "dataset_labels_filename": "dataset/CelebA/list_attr_celeba.txt",
      "dataset_image_folder": "dataset/CelebA/Img",
      "uses_landmarks": 1,
      "uses_bounding_boxes": 0,
      "uses_automatic_landmarks" : 0,
      "landmarks_filename": "dataset/CelebA/list_landmarks_celeba.txt",
      "bounding_boxes_filename": "dataset/CelebA/list_bbox_celeba.txt"
    },
    "dataloader": {
      "batch_size": 32,
      "shuffle": "True",
      "num_workers": 8,
      "prefetch_factor": 10
    },
    "transformation": {
      "save_transformation_image": {
        "enabled": 1,
        "frequency": 10
      },
      "crop_size": {
        "x": 224,
        "y": 224
      },
      "scale_jitter": {
        "enabled": 0,
        "normal_distribution": {
          "mean": 1,
          "std": 0.1
        }
      },
      "angle_jitter": {
        "enabled": 1,
        "normal_distribution": {
          "mean": 0,
          "std": 20
        }
      },
      "shift_jitter": {
        "enabled": 0,
        "normal_distribution": {
          "mean": 0,
          "std": 0.05
        }
      },
      "mirror": {
        "enabled": 0,
        "probability": 0.5
      },
      "gaussian_blur": {
        "enabled": 0,
        "normal_distribution": {
          "mean": 0,
          "std": 0.3
        }
      },
      "gamma": {
        "enabled": 0,
        "normal_distribution": {
          "mean": 0,
          "std": 1
        }
      },
      "temperature": {
        "enabled": 0
      }
    }
  },
  "evaluation": {
    "test_labels_pickle_filename": "test_labels.pkl",
    "test_landmarks_pickle_filename": "test_landmarks.pkl",
    "test_bounding_boxes_filename": "test_bounding_boxes.pkl",
    "train_majority_pickle_filename": "train_majority.pkl",
    "model_weights_filename": "best-2.pt",
    "qualitative": {
      "enabled": 1,
      "number_of_images_per_row": 3
    },
    "quantitative": {
      "enabled": 1
    },
    "dataloader": {
      "batch_size": 6,
      "num_workers": 1
    }
  }
}