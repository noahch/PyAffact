{
  "config": {
    "name": "basic_config"
  },
  "basic": {
    "cuda_device_name": "cuda:0",
    "experiment_name": "baseline on own network",
    "experiment_description": "sample description",
    "model": "resnet_51",
    "pretrained": 1,
    "result_directory": "/home/yves/Desktop/uzh/affact/results",
    "result_directory_name": "2020-12-01-13-17-07-baseline",
    "enable_wand_reporting": 0
  },
  "training": {
    "epochs": 2,
    "save_frequency": 10,
    "optimizer": {
      "type": "SGD",
      "learning_rate": 0.1,
      "momentum": 0
    },
    "criterion": {
      "type": "BCEWithLogitsLoss"
    },
    "lr_scheduler": {
      "type": "ReduceLROnPlateau",
      "step_size": 7,
      "gamma": 0.1,
      "patience": 4
    },
    "dropout": 0.2
  },
  "preprocessing": {
    "dataset": {
      "partition_filename": "dataset/CelebA/list_eval_partition.txt",
      "dataset_labels_filename": "dataset/CelebA/list_attr_celeba.txt",
      "dataset_image_folder": "dataset/CelebA/Img",
      "bounding_box_mode": 0,
      "landmarks_filename": "dataset/CelebA/list_landmarks_celeba.txt",
      "bounding_boxes_filename": "dataset/CelebA/list_bbox_celeba.txt"
    },
    "dataloader": {
      "batch_size": 32,
      "shuffle": "True",
      "num_workers": 8,
      "prefetch_factor": 10
    },
    "transformation": {
      "save_transformation_image": {
        "enabled": 1,
        "frequency": 10
      },
      "crop_size": {
        "x": 224,
        "y": 224
      },
      "scale_jitter": {
        "enabled": 0,
        "normal_distribution": {
          "mean": 1,
          "std": 0.1
        }
      },
      "angle_jitter": {
        "enabled": 1,
        "normal_distribution": {
          "mean": 0,
          "std": 20
        }
      },
      "shift_jitter": {
        "enabled": 0,
        "normal_distribution": {
          "mean": 0,
          "std": 0.05
        }
      },
      "mirror": {
        "enabled": 0,
        "probability": 0.5
      },
      "gaussian_blur": {
        "enabled": 0,
        "normal_distribution": {
          "mean": 0,
          "std": 0.3
        }
      },
      "gamma": {
        "enabled": 0,
        "normal_distribution": {
          "mean": 0,
          "std": 1
        }
      },
      "temperature": {
        "enabled": 0
      }
    }
  }
}