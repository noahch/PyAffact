{
  "dataset_result_folder": "evaluation/dataset/testsetAZ",
  "dataset": {
    "partition_filename": "dataset/CelebA/list_eval_partition.txt",
    "dataset_labels_filename": "dataset/CelebA/list_attr_celeba.txt",
    "landmarks_filename": "dataset/CelebA/list_landmarks_celeba.txt",
    "bounding_boxes_filename": "dataset/CelebA/list_bbox_celeba.txt",
    "dataset_image_folder": "dataset/CelebA/Img",
    "bounding_box_mode": 2,
    "bounding_box_scale": 2.5
  },
  "preprocessing": {
    "save_transformation_image": {
      "enabled": 0,
      "frequency": 10
    },
    "transformation": {
      "crop_size": {
        "x": 224,
        "y": 224
      },
      "scale_jitter": {
        "enabled": 0,
        "normal_distribution": {
          "mean": 0,
          "std": 0.1
        }
      },
      "angle_jitter": {
        "enabled": 0,
        "normal_distribution": {
          "mean": 0,
          "std": 20
        }
      },
      "shift_jitter": {
        "enabled": 0,
        "normal_distribution": {
          "mean": 0,
          "std": 0.05
        }
      },
      "mirror": {
        "enabled": 0,
        "probability": 0.5
      },
      "gaussian_blur": {
        "enabled": 0,
        "normal_distribution": {
          "mean": 0,
          "std": 0.3
        }
      },
      "gamma": {
        "enabled": 0,
        "normal_distribution": {
          "mean": 0,
          "std": 1
        }
      },
      "temperature": {
        "enabled": 0
      }
    }
  }
}