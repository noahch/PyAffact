{
  "config": {
    "name": "baseline_config_rolf"
  },
  "basic": {
    "cuda_device_name": "cuda:3",
    "experiment_name": "baselineSGDpretrainedLR01ReduceOnPlateau",
    "experiment_description": "Baseline Model. ResNet50, pretrained, no perturbation, uses bounding boxes",
    "model": "resnet_51",
    "pretrained": 1,
    "result_directory": "/local/scratch/noahAndYves/affact_experiment_results",
    "result_directory_name": "",
    "mode": "trainEval",
    "enable_wand_reporting": 1
  },
  "training": {
    "epochs": 50,
    "save_frequency": 5,
    "optimizer": {
      "type": "SGD",
      "learning_rate": 0.1,
      "momentum": 0
    },
    "criterion": {
      "type": "BCEWithLogitsLoss"
    },
    "lr_scheduler": {
      "type": "ReduceLROnPlateau",
      "step_size": 5,
      "gamma": 0.1,
      "patience": 4
    }
  },
  "preprocessing": {
    "dataset": {
      "use_partition_file": 1,
      "partition_filename": "/local/scratch/datasets/CelebA/protocol/list_eval_partition.txt",
      "train_fraction": 0.5,
      "val_fraction": 0.2,
      "test_fraction": 0.3,
      "number_of_samples": -1,
      "dataset_labels_filename": "/local/scratch/datasets/CelebA/protocol/list_attr_celeba.txt",
      "dataset_image_folder": "/local/scratch/datasets/CelebA/img_celeba",
      "uses_landmarks": 0,
      "uses_bounding_boxes": 1,
      "landmarks_filename": "/local/scratch/datasets/CelebA/protocol/list_landmarks_celeba.txt",
      "bounding_boxes_filename": "/local/scratch/datasets/CelebA/protocol/list_bbox_celeba.txt"
    },
    "dataloader": {
      "batch_size": 64,
      "shuffle": "True",
      "num_workers": 32,
      "prefetch_factor": 10
    },
    "transformation": {
      "use_affact_transformator": 1,
      "save_transformation_image": {
        "enabled": 1,
        "frequency": 100000
      },
      "crop_size": {
        "x": 224,
        "y": 224
      },
      "random_bounding_box": {
        "enabled": 0
      },
      "scale_jitter": {
        "enabled": 0,
        "normal_distribution": {
          "mean": 1,
          "std": 0.1
        }
      },
      "angle_jitter": {
        "enabled": 0,
        "normal_distribution": {
          "mean": 0,
          "std": 20
        }
      },
      "shift_jitter": {
        "enabled": 0,
        "normal_distribution": {
          "mean": 0,
          "std": 0.05
        }
      },
      "mirror": {
        "enabled": 0,
        "probability": 0.5
      },
      "gaussian_blur": {
        "enabled": 0,
        "normal_distribution": {
          "mean": 0,
          "std": 0.3
        }
      },
      "gamma": {
        "enabled": 0,
        "normal_distribution": {
          "mean": 0,
          "std": 1
        }
      }
    }
  },
  "evaluation": {
    "test_labels_pickle_filename": "test_labels.pkl",
    "test_landmarks_pickle_filename": "test_landmarks.pkl",
    "test_bounding_boxes_filename": "test_bounding_boxes.pkl",
    "train_majority_pickle_filename": "train_majority.pkl",
    "model_weights_filename": "best-2.pt",
    "qualitative": {
      "enabled": 1,
      "number_of_images_per_row": 3
    },
    "quantitative": {
      "enabled": 1
    },
    "dataloader": {
      "batch_size": 6,
      "num_workers": 1
    }
  }
}